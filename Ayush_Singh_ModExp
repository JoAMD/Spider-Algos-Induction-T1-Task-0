#include <iostream>

using namespace std;
int modx(int a,int b,int m)
{
    if (b==0) return 1;
    else if (b%2==0) 
    {
        int y=modx(a,b/2,m);
        return (y*y)%m;
    }
    else
    {
        return ((a%m)*modx(a,b-1,m))%m;
    }
}

int main()
{
    int a,b,m;
    cout<<"Enter a,b,m - ";
    cin>>a>>b>>m;
    cout<<modx(a,b,m);
    return 0;
}
