#include <stdio.h>
int expo(int ,int ,int);
int main() 
{
    int a,b,m,r;
    //a-base element,b-exponent,m-modulo,r-remainder
    scanf("%d %d %d",&a,&b,&m);
    r=expo(a,b,m);
    printf("%d",r);
    return 0;
    }
int expo(int a,int b,int m)
{int c;
    if(b==0)
    return 1;
    else 
    if(b%2==0)
    {c=expo(a,b/2,m);
    return (c*c)%m;
    }
    else
    return (a*expo(a,b-1,m))%m;
}
    
